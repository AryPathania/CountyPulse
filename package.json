{
  "private": true,
  "name": "county-pulse",
  "version": "0.1.0",
  "workspaces": ["packages/*"],
  "scripts": {
    "dev": "pnpm --filter @county-pulse/ui dev",
    "build": "pnpm -r build",
    "deploy": "supabase functions deploy",
